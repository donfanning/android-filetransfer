{
    "release": "./dist/assets",
    "process": [
        {
            "name": "bundle",
            "scripts": [],
            "styles": [
                "./assets/css/reset*.{scss,sass,css}",
                "./assets/css/default*.{scss,sass,css}",
                "./assets/css/**/*.{scss,sass,css}"
            ]
        }, {
            "name": "dependencies",
            "scripts": [
                "./assets/vendor/jquery/dist/jquery.min.js",
                "./assets/js/global*.js",
                "./assets/js/helpers*.js",
                "./assets/js/**/*.js"
            ],
            "styles": []
        }
    ],
    "copy": {
        "./dist/assets/fonts": [
            "./assets/fonts/**/*"
        ],
        "./dist/assets/img": [
            "./assets/img/**/*.{jpeg,jpg,png,gif,svg}"
        ]
    }
}
